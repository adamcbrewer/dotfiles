# Install native apps
# Usage: `brew bundle Caskfile`

install caskroom/cask/brew-cask
tap -v caskroom/versions

# QuickLook Plugins
# source: https://github.com/sindresorhus/quick-look-plugins
brew cask install -v qlcolorcode
brew cask install -v qlstephen
brew cask install -v qlmarkdown
brew cask install -v quicklook-json
brew cask install -v qlprettypatch
brew cask install -v quicklook-csv
brew cask install -v betterzipql
brew cask install -v webp-quicklook
brew cask install -v suspicious-package
